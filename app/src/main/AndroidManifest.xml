<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.uia">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/logo"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/logo"
            android:supportsRtl="true"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:usesCleartextTraffic="true">
        <uses-library
                android:name="org.apache.http.legacy"
                android:required="false"/>
        <activity android:name=".InsertActivity">
        </activity>
        <activity android:name=".StorageActivity">
        </activity>
        <activity android:name=".CustomIndexActivity">
        </activity>
        <activity
                android:name=".HobbyActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".DiscussActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".NeedActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".FeelingsActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".DrinkActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".PhrasesActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".SnackActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".FoodActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".NecessitiesActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".DummyGenerated"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".MainActivity"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>